{
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "Api": {
    "UseAzureIntegration": true,
    "Uploader": [
      {
        "ResponseType": "Json", 
        "WebBasePath": "image",
        "LocalBasePath": "C:\\temp\\ImageFolder",
        "FileExtensions": [
          ".png",
          ".jpg"
        ],
        "MaxFileSize": 1,
        "MaxFolderSize": 1000,
        "ApiKey": "imageApiKey"
      },
      {
        "ResponseType": "Redirect",
        "WebBasePath": "file",
        "LocalBasePath": "C:\\temp\\FileFolder",
        "FileExtensions": [
          "*"
        ],
        "MaxFileSize": 10,
        "MaxFolderSize": 1000,
        "ApiKey": "fileApiKey"
      },
      {
        "WebBasePath": "text",
        "LocalBasePath": "C:\\temp\\TextFolder",
        "FileExtensions": [
          ".txt"
        ],
        "MaxFileSize": 1,
        "MaxFolderSize": 1000,
        "ApiKey": "textApiKey"
      }
    ]
  }
}
